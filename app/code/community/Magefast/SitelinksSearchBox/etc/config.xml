<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <Magefast_SitelinksSearchBox>
            <version>1.1.0</version>
        </Magefast_SitelinksSearchBox>
    </modules>
    <global>
        <blocks>
            <sitelinkssearchbox>
                <class>Magefast_SitelinksSearchBox_Block</class>
            </sitelinkssearchbox>
        </blocks>
        <helpers>
            <sitelinkssearchbox>
                <class>Magefast_SitelinksSearchBox_Helper</class>
            </sitelinkssearchbox>
        </helpers>
    </global>
    <frontend>
        <layout>
            <updates>
                <sitelinkssearchbox module="Magefast_SitelinksSearchBox">
                    <file>sitelinkssearchbox.xml</file>
                </sitelinkssearchbox>
            </updates>
        </layout>
    </frontend>
    <default>
        <sitelinkssearchbox>
            <settings>
                <enabled>1</enabled>
            </settings>
        </sitelinkssearchbox>
    </default>
</config>